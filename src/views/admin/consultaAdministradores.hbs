<input class="buscar form-control" id="myInput" type="text" placeholder="Buscar">
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Usuario</th>
            <th scope="col">Contrase√±a</th>

        </tr>
    </thead>
    {{#each rows}}
    <tbody id="myTable" <tr>
        <td>{{id_admin}}</td>
        <td>{{us_admin}}</td>
        <td>{{pas_admin}}</td>
        <td>
            <form action="/borrarAdministrador/{{id_admin}}" method="post">
                <button type="submit" class="btn btn-danger">Borrar</button>
            </form>
        </td>
        <td>
            <form action="/modAdministrador/{{id_admin}}" method="post">
                <button type="submit" class="btn btn-success">Modificar</button>
            </form>
        </td>
        </tr>
        {{else}}
        <tr>
            <td>---</td>
            <td>---</td>
            <td>---</td>
            <td>---</td>
            <td>---</td>
            <td>---</td>
            <td>---</td>
        </tr>
        {{/each}}
    </tbody>
</table>
<script>
    $(document).ready(function () {
        $("#myInput").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#myTable tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });
    });
</script>